Algoritmo Ejercicio_39
	Imprimir "Ingresa tu sueldo"
	Leer sueldo
	Imprimir "Ingresa tus años de antiguedad"
	Leer anti
	
	si anti >4 Entonces
		
		sueldoBono = (sueldo * 0.25)
		sueldoNeto = (sueldoBono + sueldo)
		
		Imprimir "Su bono navideño es de:$", sueldoNeto
		
	SiNo
		
		si sueldo <2000 Entonces
			
			sueldoBono = (sueldo * 0.25)
			sueldoNeto = (sueldoBono + sueldo)
		
			
		FinSi
		
		si anti <4  Entonces
			
			sueldoBono = (sueldo * 0.20)
			suledoNeto = (sueldoBono + sueldo)
			
			Imprimir "Su bono navideño es de:$", sueldoNeto
		SiNo
			si sueldo>2000 Entonces
				
				sueldoBono = (sueldo * 0.20)
				suledoNeto = (sueldoBono + sueldo)
				
			FinSi
		FinSi
	FinSi

FinAlgoritmo
